<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Baidu</ShortName>
<InputEncoding>UTF-8</InputEncoding>
<Image width="16" height="16">data:image/png;base64,</Image>
<SearchForm>http://www.baidu.com/</SearchForm>
<Url type="text/html" method="GET" template="http://www.baidu.com/s?wd={searchTerms}&amp;ie=UTF-8">
</Url>
<Url type="application/x-suggestions+json" method="GET" template="http://suggestion.baidu.com/su">
  <Param name="wd" value="{searchTerms}"/>
  <Param name="action" value="opensearch"/>
  <Param name="ie" value="UTF-8"/>
  <Param name="from" value="firefox"/>
</Url>
</SearchPlugin>
